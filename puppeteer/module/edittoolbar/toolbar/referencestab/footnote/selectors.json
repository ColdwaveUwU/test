        },
        "GO_TO_ENDNOTE": {
            "NEXT": "#id-menu-goto-endnote-next-0",
            "PREVIOUS": "#id-menu-goto-endnote-prev-0"
        }
    },
    "MODAL_WINDOW": {
        "NOTES_SETTINGS": {
            "WINDOW": "#window-note-settings",
