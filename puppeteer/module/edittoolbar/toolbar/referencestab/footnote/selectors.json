                "selector": "#note-settings-combo-footnote",
                "elementsSelector": "#note-settings-combo-footnote ul li",
                "descriptionSelector": "a",
                "elementsValue": []
            },
            "ENDNOTE_LOCATION_COMBO": {
                "selector": "#note-settings-combo-endnote",
                "elementsSelector": "#note-settings-combo-endnote ul li",
                "descriptionSelector": "a",
                "elementsValue": []
