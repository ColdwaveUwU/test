            "elementsSelector": "[id^='id-toolbar-toolbar__icon'][id*='btn-notes'] ul.dropdown-menu li",
            "descriptionSelector": "a",
            "idSelector": "a",
            "elementsValue": []
        },
        "GO_TO_FOOTNOTE": {
